{
  "publisher": "oeyoews",
  "name": "usewiki2",
  "displayName": "usewiki2",
  "version": "0.5.1",
  "private": true,
  "packageManager": "pnpm@9.0.6",
  "description": "",
  "keywords": [
    "TiddlyWiki"
  ],
  "author": {
    "name": "oeyoews"
  },
  "pricing": "Free",
  "license": "MIT",
  "funding": "https://github.com/sponsors/oeyoews",
  "homepage": "https://github.com/oeyoews/usewiki2-vscode#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/oeyoews/usewiki2-vscode#readme"
  },
  "categories": [
    "Education"
  ],
  "main": "./dist/index.js",
  "icon": "res/tiddlywiki01.png",
  "engines": {
    "vscode": "^1.94.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "title": "UseWiki2 太微",
      "properties": {
        "usewiki2.defaultTag": {
          "type": "string",
          "default": "Journalp",
          "description": "默认标签"
        },
        "usewiki2.defaultUsername": {
          "type": "string",
          "default": "",
          "description": "默认用户名"
        },
        "usewiki2.ip": {
          "order": 0,
          "type": "string",
          "default": "127.0.0.1",
          "markdownDescription": "你的TiddlyWiki地址 (请确定你已经启动了[太微](https://tiddlywiki.com/#Installing%20TiddlyWiki%20on%20Node.js))"
        },
        "usewiki2.port": {
          "order": 1,
          "type": "number",
          "default": 8080,
          "description": "端口 (请确定你已经启动了太微)"
        },
        "enableHttps": {
          "order": 2,
          "type": "boolean",
          "default": false,
          "markdownDescription": "Enable or disable HTTPS"
        },
        "usewiki2.type": {
          "order": 3,
          "type": "string",
          "default": "text/vnd.tiddlywiki",
          "enum": [
            "text/markdown",
            "text/vnd.tiddlywiki"
          ],
          "enumItemLabels": [
            "Markdown",
            "WikiText"
          ],
          "markdownEnumDescriptions": [
            "use Markdown",
            "use WikiText"
          ],
          "description": "选择要使用的文本格式 (Markdown 或 TiddlyWiki)"
        }
      }
    },
    "menus": {
      "view/title": [
        {
          "command": "usewiki2.openwiki",
          "group": "navigation",
          "when": "view == usewiki2"
        },
        {
          "command": "usewiki2.opensettings",
          "group": "navigation",
          "when": "view == usewiki2"
        },
        {
          "command": "usewiki2.wikiinfo",
          "group": "navigation",
          "when": "view == usewiki2"
        },
        {
          "command": "usewiki2.refreshwiki",
          "group": "navigation",
          "when": "view == usewiki2"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "usewiki2Container",
          "title": "usewiki",
          "icon": "res/tw.svg"
        }
      ]
    },
    "views": {
      "usewiki2Container": [
        {
          "id": "usewiki2",
          "type": "webview",
          "name": "TiddlyWiki"
        }
      ]
    },
    "commands": [
      {
        "command": "usewiki2.tiddlywiki",
        "title": "UseWiki2: Journal 太微之书"
      },
      {
        "command": "usewiki2.openwiki",
        "title": "Open 打开太微",
        "icon": "$(info)"
      },
      {
        "command": "usewiki2.wikiinfo",
        "title": "太微详情",
        "icon": "res/tw.svg"
      },
      {
        "command": "usewiki2.refreshwiki",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "usewiki2.opensettings",
        "title": "设置",
        "icon": "$(gear)"
      }
    ]
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "package": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.2.1",
    "@antfu/ni": "^0.21.12",
    "@types/node": "^20.14.11",
    "@types/vscode": "^1.91.0",
    "@vscode/vsce": "3.2.0",
    "bumpp": "^9.4.1",
    "eslint": "^8.57.0",
    "esno": "^4.7.0",
    "pnpm": "^8.15.9",
    "rimraf": "^5.0.9",
    "tsup": "^7.2.0",
    "typescript": "^5.5.4",
    "vite": "^4.5.3",
    "vitest": "^0.34.6"
  },
  "files": [
    "dist",
    "res",
    "react-dist",
    "LICENSE"
  ]
}