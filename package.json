{
  "publisher": "oeyoews",
  "name": "usewiki2",
  "displayName": "usewiki2",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.10.5",
  "description": "",
  "author": {
    "name": "oeyoews"
  },
  "pricing": "Free",
  "license": "MIT",
  "funding": "https://github.com/sponsors/oeyoews",
  "homepage": "https://github.com/oeyoews/usewiki2-vscode#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/oeyoews/usewiki2-vscode#readme"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/tiddlywiki01.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.83.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "usewiki2",
          "title": "太微",
          "icon": "res/tiddlywiki01.png"
        }
      ]
    },
    "views": {
      "usewiki2": [
        {
          "id": "usewiki2-info",
          "name": "信息面板"
        }
      ]
    },
    "commands": [
      {
        "command": "usewiki2.tiddlywiki",
        "title": "TiddlyWiki"
      }
    ]
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.1",
    "@antfu/ni": "^0.21.9",
    "@types/node": "^20.9.0",
    "@types/vscode": "^1.84.1",
    "@vscode/vsce": "^2.22.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.53.0",
    "esno": "^4.0.0",
    "pnpm": "^8.10.5",
    "rimraf": "^5.0.5",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  }
}